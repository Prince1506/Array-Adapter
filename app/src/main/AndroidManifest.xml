<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="soni.prince.android_apis.arrayadapter">


    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name">
        <activity android:name="soni.prince.android_apis.arrayadapter.LanguageListActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <provider
            android:name="soni.prince.android_apis.arrayadapter.LanguageContentProvider"
            android:authorities="custom.content_provider.LanguageContentProvider"
            android:exported="false" />
    </application>
</manifest>